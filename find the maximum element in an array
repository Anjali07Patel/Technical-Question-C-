#include<bits/stdc++.h>
using namespace std;

int maximum(int arr[],int n)
{
    int ans=INT_MIN;
    for(int i=0;i<n;i++)
    {
         ans=max(arr[i],ans);
    }
    return ans;
}

int main()
{
   int n;
   cin>>n;
   int arr[n];
   for(int i=0;i<n;i++)
   {
       cin>>arr[i];
   }
   cout<<maximum(arr,n);
   return 0;
}
