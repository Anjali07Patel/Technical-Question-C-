#include<bits/stdc++.h>
using namespace std;
int check(string s1)
{
    int vow=0;
    int cons=0;
    for(int i=0;i<s1.size();i++)
    {
        if(s1[i]=='a'||s1[i]=='e'||s1[i]=='i'||s1[i]=='o'||s1[i]=='u')
        {
            vow++;
        }
        else
        {
            cons++;
        }
    }
    cout<<vow<<" "<<cons<<" ";
}
int main()
{
    string s1;
    getline(cin,s1);
    check(s1);
}
