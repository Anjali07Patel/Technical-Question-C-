#include<bits/stdc++.h>
using namespace std;
float average(int n,int arr[])
{
    int sum=0;
    for(int i=0;i<n;i++)
    {
        sum=sum+arr[i];
    }
    float avg=sum/n;
    return avg;
}
int main()
{
   int n;
   cin>>n;
   int arr[n];
   for(int i=0;i<n;i++)
   {
       cin>>arr[i];
   }
   cout<<average(n,arr);
   return 0;
}
